rnorm(5)
rnorm(5,10,8)
rnorm(n=5,sd=8,mean=100)
rnorm(n=5,sd=8)

seq(10,20,3)
seq(from=10,to=20,by=3)
seq(from=10,to=20,length.out=100)

x<-c("a","b","c")
seq(from=10,to=20,along.with=x)

rep(5,10)
rep(5:6)
rep(5:6,times=10)
rep(5:6,each=10)
rep(x,each=5)
rep(x,times=5)

A<- seq(from=10, to=20,along.with=x)
A
sqrt(A)


B<-sqrt(A)
B
